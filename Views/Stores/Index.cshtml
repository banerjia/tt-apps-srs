@model IEnumerable<tt_apps_srs.Models.ESIndex_Store_Document>
@{
}

<table class="table table-condensed">
    <thead>
        <tr>
            <td>Options</td>
            <th scope="col">Retailer</th>
            <th scope="col">Name</th>
            <th scope="col">City</th>
            <th scope="col">State</th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td colspan="3"><a asp-action="Index" asp-route-page="@((ushort)ViewData["page"] - 1)">Previous</a></td>
            <td colspan="2"><a asp-action="Index" asp-route-page="@((ushort)ViewData["page"] + 1)">Next</a></td>
        </tr>
    </tfoot>
    <tbody>
        @foreach (var store in Model)
        {
            <tr>
                <td>
                    <a asp-action="Details" asp-route-id="@store.Id">Details</a>
                    <a asp-action="Edit" asp-route-id="@store.Id">Edit</a>
                </td>
                <td>@store.Retailer.Name</td>
                <td>@store.Name</td>
                <td>@store.City</td>
                <td>@store.State</td>
            </tr>

        }

    </tbody>
</table>
<a asp-action="New" class="btn btn-success">Add New Store</a>
